{
  "expo": {
    "name": "Rami Multivendor Restaurant",
    "scheme": "rami-multivendor-restaurant",
    "version": "1.0.22",
    "slug": "rami-multivendor-restaurant",
    "owner": "rami_multivendor",
    "currentFullName": "@rami_multivendor/rami-multivendor-restaurant",
    "originalFullName": "@rami_multivendor/rami-multivendor-restaurant",
    "privacy": "public",
    "plugins": [
      [
